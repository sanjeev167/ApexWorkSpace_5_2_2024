package com.nus.pvt.master.repo;

import org.springframework.data.repository.CrudRepository;
import org.springframework.stereotype.Repository;

import com.nus.pvt.master.entities.ProjectCodeMstr;
import java.util.List;


/**
 * @Author: SanjeevKumar<br>
 * @Date: 17-Jan-2025<br>
 * @Time: 4:57:20 pm<br>
 * @Objective: <br>
 */
@Repository
public interface ProjectCodeMstrRepo extends CrudRepository<ProjectCodeMstr, Integer> {

	List<ProjectCodeMstr> findAllByActiveC(String activeC);
}
