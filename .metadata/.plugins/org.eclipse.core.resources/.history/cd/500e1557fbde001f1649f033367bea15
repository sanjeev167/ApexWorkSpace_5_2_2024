package com.nus.fileupload.repo;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;

import com.nus.fileupload.entities.ProjectMonthlyPlcc;

/**
 * @Author: SanjeevKumar<br>
 * @Date: 26-Jan-2025<br>
 * @Time: 2:10:29 am<br>
 * @Objective: <br>
 */
public interface ProjectMonthlyPlccRepo extends JpaRepository<ProjectMonthlyPlcc, Integer>{
	//@Query(value="DELETE FROM ProjectMonthlyPlcc WHERE plccMonth>=?1 and plccYear=?2", nativeQuery = true)
	//@Query(value = "INSERT INTO hproject_monthly_plcc SELECT * FROM project_monthly_plcc org WHERE org.plcc_month>=?1 and org.plcc_year=?2", nativeQuery = true)
	//void moveAllRecordsFromMainToHistoryTable(int month, int year);
	
	
	@Query(with delta as (
			  delete from project_monthly_plcc where WHERE plcc_month>=?1 and plcc_year=?2
			  returning *
			)
			insert into hproject_monthly_plcc
			select * from delta)
	void moveAllRecordsFromMainToHistoryTable(int month, int year);

}
