package com.nus.fileupload.repo;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import com.nus.fileupload.entities.CustomerSatisfaction;

/**
 * @Author:SanjeevKumar<br>
 * @Date:24-Jan-2025<br>
 * @Time:10:49:52 pm<br>
 * @Objective: <br>
 */
@Repository
public interface MonthlyCustomerSatisfactionRepo extends JpaRepository<CustomerSatisfaction, Integer>{

	@Query(value = "SELECT * FROM customer_satisfaction WHERE csatMonth =?1 AND csatYear =?2", nativeQuery = true)
	List<CustomerSatisfaction> getAllByMonthAndYear(Integer month, Integer year);

}
