package com.nus.pvt.master.service;

import java.util.List;

import org.springframework.security.core.context.SecurityContextHolder;

import com.nus.pvt.master.model.UserTypeModel;
import com.nus.sec.entities.UserType;
import com.nus.sec.service.CustomUserDetails;

/**
 * @Author: SanjeevKumar<br>
 * @Date: 17-Jan-2025<br>
 * @Time: 4:28:55 pm<br>
 * @Objective: <br>
 */
public interface UserTypeService {
	public String loginId = ((CustomUserDetails) SecurityContextHolder.getContext().getAuthentication().getPrincipal()).getEmail();
	public UserType addUserType(UserTypeModel userTypeModel);
	public UserType updateUserType(UserTypeModel userTypeModel);
	public boolean deleteUserTypeByRecordId(Integer recordId);
	public UserType getUserTypeByRecordId(Integer recordId);
	public List<UserType> getAllUserTypes();

}
